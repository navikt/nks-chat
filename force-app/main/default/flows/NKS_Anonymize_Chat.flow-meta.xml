<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>61.0</apiVersion>
    <description>Delete chat and related case if account is null and the chat is older than 2 days
        and/or chat queue is Sosial or Gjeldsr√•dgivning</description>
    <environments>Default</environments>
    <interviewLabel>NKS {!$Flow.CurrentDateTime}</interviewLabel>
    <label>NKS Anonymize Chat</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Remove_description_and_body</name>
        <label>Remove description and body</label>
        <locationX>176</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Body</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>NKS_Conversation__c</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Remove_description_and_conversation</name>
        <label>Remove description and conversation</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <connector>
            <targetReference>Remove_description_and_body</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>SourceId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Description</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>NKS_Conversation__c</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <object>Case</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Remove_description_and_conversation</targetReference>
        </connector>
        <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
        <filterLogic>1 AND 2 AND 3 AND (4 OR 5 OR 6 OR 7)</filterLogic>
        <filters>
            <field>AccountId</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Completed</stringValue>
            </value>
        </filters>
        <filters>
            <field>CRM_Authentication_Status__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Completed</stringValue>
            </value>
        </filters>
        <filters>
            <field>NKS_Chat_Queue__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Sosiale Tjenester</stringValue>
            </value>
        </filters>
        <filters>
            <field>NKS_Chat_Queue__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Sosiale Tjenester EN</stringValue>
            </value>
        </filters>
        <filters>
            <field>NKS_Chat_Queue__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Gjeldsveiledning</stringValue>
            </value>
        </filters>
        <filters>
            <field>NKS_Chat_Queue__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Gjeldsveiledning EN</stringValue>
            </value>
        </filters>
        <object>LiveChatTranscript</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>